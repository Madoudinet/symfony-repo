{% extends "base.html.twig" %}

{% block title %}Ajouter une voiture{% endblock %}


{% block body %}

    <h1 class="text-center">Ajouter une voiture</h1>

    <div class="container">

    {{ form_start(formVoiture)}}
        {{ form_row(formVoiture.marque)}}
        {{ form_row(formVoiture.modele)}}
        {{ form_row(formVoiture.prix)}}
        {{ form_row(formVoiture.description)}}
        <button type="submit" class="btn btn-primary">Ajouter</button>
    {{ form_end(formVoiture)}}
    </div>

{% endblock %}